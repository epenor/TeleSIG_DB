CREATE TABLE  (
 Id_tienda     VarChar(8) NOT NULL,
 nombre_tienda VarChar(50) NOT NULL,
 direcc_tienda VarChar(150),
 ciudad        VarChar(50),
 estado        VarChar(50),
 pais          VarChar(50),
 cod_postal    VarChar(6),
 terminos      VarChar(150), 
 PRIMARY KEY (Id_tienda));

CREATE TABLE Ventas (
 id_tienda VarChar(8) NOT NULL,
 num_orden VarChar(20) NOT NULL,
 fecha     Date NOT NULL, 
 PRIMARY KEY (id_tienda)) ;
 

